@model MvcSiteMapMenuCollection

<div id="Sidebar">
    <div id="SidebarSearch">
        @Html.TextBox("SearchInput", null, new { placeholder = Template.Resources.Shared.Resources.Search + "..." })
    </div>
    <div id="SidebarNavigation">
        <ul id="SidebarNavigationList">
            @Html.DisplayForModel()
        </ul>
    </div>
</div>
@Scripts.Render("~/Scripts/Sidebar/Bundle")
