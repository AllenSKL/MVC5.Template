@model ProfileDeleteView

@using (Html.FormWidgetBox(LinkAction.Edit))
{
    using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        using (Html.FormGroup())
        {
            @Html.FormLabelFor(model => model.Password)
            @Html.FormPasswordFor(model => model.Password)
            @Html.FormValidationFor(model => model.Password)
        }
        using (Html.FormGroup())
        {
            using (Html.FormActions())
            {
                @Html.Submit()
            }
        }
    }
}
